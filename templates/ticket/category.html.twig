{% extends 'base.html.twig' %}

{% block title %}{{tickets[0].getCategory().getTitle()}} - Les tickets{% endblock %}

{% block body %}
    <h1 class="my-3">{{tickets[0].getCategory().getTitle()}} - Les tickets</h1>
    <div class="list-group">
        {% for ticket in tickets %}
            {% include '_partials/ticket/row.html.twig' with { ticket: ticket } only %}
        {% endfor %}
    </div>
{% endblock %}
